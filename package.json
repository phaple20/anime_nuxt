{
  "name": "anime",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "yarn beauty && yarn lint && cross-env NODE_ENV=development nodemon server/start.js --watch server --exec babel-node",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint:js": "eslint --ext \".js,.vue\" --ignore-path .gitignore .",
    "old_lint": "yarn lint:js",
    "lint": "eslint --fix --ext .js --ext .vue ./",
    "beauty": "prettier --ignore-path .gitignore --config ./.prettierrc.yaml --write \"**/*.{js,bue,vue,css,less}\""
  },
  "dependencies": {
    "ant-design-vue": "^1.7.4",
    "core-js": "^3.9.1",
    "cross-env": "^7.0.3",
    "koa": "^2.13.1",
    "koa-bodyparser": "^4.3.0",
    "nuxt": "^2.15.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.3",
    "@babel/core": "^7.14.3",
    "@babel/node": "^7.14.2",
    "@babel/preset-env": "^7.14.4",
    "@nuxtjs/eslint-config": "^6.0.0",
    "@nuxtjs/eslint-module": "^3.0.2",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.1.0",
    "babel-preset-es2015": "^6.24.1",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^7.7.0",
    "prettier": "^2.2.1"
  },
  "config": {
    "nuxt": {
      "port": "9500"
    }
  }
}
